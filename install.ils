(let ()
    (lambda (t_path)
        (if (shell (strcat "cd " t_path "; make ffi"))
            (if (isFile "skillFFI")
                (if (shell "chmod +x skillFFI")
                    (printf "SKILLFFI - Successfuly built and installed.\n")
                    (error "SKILLFFI - Failed to make skillFFI executable."))
                (error "SKILLFFI - Failed to build SKILLFFI."))
            (error "SKILLFFI - Failed during make.")))
)
